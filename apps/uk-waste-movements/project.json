{
  "name": "uk-waste-movements",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/uk-waste-movements",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/uk-waste-movements",
        "outputPath": "dist/apps/uk-waste-movements",
        "generateLockfile": true
      },
      "configurations": {
        "development": {
          "outputPath": "apps/uk-waste-movements"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "uk-waste-movements:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "uk-waste-movements:build:development",
          "dev": true,
          "port": 4201
        },
        "production": {
          "buildTarget": "uk-waste-movements:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "uk-waste-movements:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/uk-waste-movements/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/uk-waste-movements/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "options": {
        "engine": "docker",
        "metadata": {
          "images": ["wts/uk-waste-movements"],
          "load": true,
          "tags": [
            "type=ref,event=branch",
            "type=ref,event=tag",
            "type=ref,event=pr",
            "type=sha,prefix=sha-"
          ]
        }
      }
    }
  },
  "tags": ["type:app"]
}
