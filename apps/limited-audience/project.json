{
  "name": "limited-audience",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/limited-audience/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/limited-audience",
        "main": "apps/limited-audience/src/main.ts",
        "tsConfig": "apps/limited-audience/tsconfig.app.json",
        "assets": ["apps/limited-audience/src/assets"],
        "webpackConfig": "apps/limited-audience/webpack.config.js",
        "generatePackageJson": true,
        "additionalEntryPoints": [
          {
            "entryName": "tracing",
            "entryPath": "apps/limited-audience/src/tracing.ts"
          }
        ]
      },
      "configurations": {
        "development": {},
        "production": {
          "optimization": true,
          "generateLockFile": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "limited-audience:build",
        "runtimeArgs": ["--require", "./dist/apps/limited-audience/tracing.js"]
      },
      "configurations": {
        "development": {
          "buildTarget": "limited-audience:build:development"
        },
        "production": {
          "buildTarget": "limited-audience:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/limited-audience/jest.config.ts"
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "options": {
        "engine": "docker",
        "metadata": {
          "images": ["wts/limited-audience"],
          "load": true,
          "tags": [
            "type=schedule",
            "type=ref,event=branch",
            "type=ref,event=tag",
            "type=ref,event=pr",
            "type=sha,prefix=sha-"
          ]
        }
      }
    }
  },
  "tags": []
}
