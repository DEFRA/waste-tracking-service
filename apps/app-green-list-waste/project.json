{
  "name": "app-green-list-waste",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-green-list-waste",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/app-green-list-waste",
        "outputPath": "dist/apps/app-green-list-waste",
        "generateLockfile": true
      },
      "configurations": {
        "development": {
          "outputPath": "apps/app-green-list-waste"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app-green-list-waste:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app-green-list-waste:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "app-green-list-waste:build:production",
          "dev": false
        }
      }
    },
    "serve-both": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx serve app-green-list-waste", "nx serve mock-gateway"],
        "parallel": true
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "app-green-list-waste:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/app-green-list-waste/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/app-green-list-waste/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "options": {
        "engine": "docker",
        "metadata": {
          "images": ["wts/app-green-list-waste"],
          "load": true,
          "tags": [
            "type=ref,event=branch",
            "type=ref,event=tag",
            "type=ref,event=pr",
            "type=sha,prefix=sha-"
          ]
        }
      }
    }
  },
  "tags": ["type:app"]
}
